{
  "name": "solid-sonner",
  "type": "module",
  "version": "0.0.0",
  "private": false,
  "packageManager": "pnpm@7.22.0",
  "description": "An opinionated toast component for Solid.",
  "author": "Robert Soriano <sorianorobertc@gmail.com>",
  "license": "MIT",
  "contributors": [],
  "homepage": "https://github.com/wobsoriano/solid-sonner#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wobsoriano/solid-sonner.git"
  },
  "bugs": {
    "url": "https://github.com/wobsoriano/solid-sonner/issues"
  },
  "keywords": [
    "solid"
  ],
  "sideEffects": false,
  "exports": {
    "solid": {
      "development": "./dist/dev.jsx",
      "import": "./dist/index.jsx"
    },
    "development": {
      "require": "./dist/dev.cjs",
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/dev.js"
      }
    },
    "require": "./dist/index.cjs",
    "import": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {},
  "files": [
    "dist"
  ],
  "browser": {},
  "scripts": {
    "dev": "vite serve dev",
    "build": "tsup",
    "test": "vitest",
    "prepublishOnly": "pnpm build",
    "update-deps": "pnpm up -Li",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "peerDependencies": {
    "solid-js": "^1.6.0"
  },
  "devDependencies": {
    "@antfu/eslint-config-ts": "^0.39.6",
    "@solidjs/testing-library": "^0.8.3",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/user-event": "^14.4.3",
    "@types/testing-library__jest-dom": "^5.14.6",
    "concurrently": "^7.6.0",
    "esbuild": "^0.17.4",
    "esbuild-plugin-solid": "^0.5.0",
    "eslint": "^8.43.0",
    "eslint-plugin-solid": "^0.12.1",
    "jsdom": "^21.1.0",
    "solid-js": "^1.7.7",
    "tsup": "^6.5.0",
    "tsup-preset-solid": "^0.1.8",
    "typescript": "^4.9.4",
    "vite": "^4.3.9",
    "vite-plugin-solid": "^2.7.0",
    "vitest": "^0.32.2"
  },
  "eslintConfig": {
    "plugins": [
      "solid"
    ],
    "extends": [
      "@antfu/eslint-config-ts",
      "plugin:solid/typescript"
    ]
  }
}
